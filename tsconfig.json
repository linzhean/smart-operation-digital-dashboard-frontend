{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["react", "react-dom", "react-router-dom"],
    "jsx": "react-jsx"
  },
  "include": [
    "src"
, "component", "pages/Home/Home.tsx", "component/Group/Group.tsx", "pages/Login/Login.tsx", "component/Pdata/Pdata.tsx", "Main.tsx", "App.tsx", "utils", "index.tsx", "react-app-env.d.ts"  ]
}
